INSERT INTO `notice` VALUES (17,'关于拟设“上海孝心护理院”的公示 ','<p style=\"text-align:center;\" align=\"center\">\r\n	<b><span style=\"font-size:16pt;font-family:宋体;\">公</span></b><b><span style=\"font-size:16pt;\">&nbsp;&nbsp;&nbsp; </span></b><b><span style=\"font-size:16pt;font-family:宋体;\">示</span></b><b><span style=\"font-size:16pt;\"></span></b>\r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p style=\"text-indent:28pt;\">\r\n	<span style=\"font-size:14pt;font-family:宋体;\">拟设“上海孝心护理院”。机构类别：护理院；执业地址：上海市金山区山阳镇红旗东路</span><span style=\"font-size:14pt;\">30</span><span style=\"font-size:14pt;font-family:宋体;\">号；诊疗科目：内科（呼吸内科、消化内科、老年病科专业、神经内科专业、心血管内科专业）、中医科（内科专业、外科专业、老年病专业、针灸科专业、推拿科专业、康复医学科专业）；康复医学科、临终关怀科、医学检验科</span><span style=\"font-size:14pt;\">(</span><span style=\"font-size:14pt;font-family:宋体;\">临床体液、血液专业</span><span style=\"font-size:14pt;\">)</span><span style=\"font-size:14pt;font-family:宋体;\">、医学影像科（</span><span style=\"font-size:14pt;\">X</span><span style=\"font-size:14pt;font-family:宋体;\">线诊断专业、超声诊断专业、心电诊断专业）。核定床位：</span><span style=\"font-size:14pt;\">98</span><span style=\"font-size:14pt;font-family:宋体;\">张；设置人：上海宸宇投资管理有限公司；符合金山区《医疗机构设置规划》。</span><span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p style=\"text-indent:28pt;\">\r\n	<span style=\"font-size:14pt;font-family:宋体;\">公示日期：</span><span style=\"font-size:14pt;\">2014</span><span style=\"font-size:14pt;font-family:宋体;\">年</span><span style=\"font-size:14pt;\">1</span><span style=\"font-size:14pt;font-family:宋体;\">月</span><span style=\"font-size:14pt;\">24</span><span style=\"font-size:14pt;font-family:宋体;\">日—</span><span style=\"font-size:14pt;\">2014</span><span style=\"font-size:14pt;font-family:宋体;\">年</span><span style=\"font-size:14pt;\">1</span><span style=\"font-size:14pt;font-family:宋体;\">月</span><span style=\"font-size:14pt;\">31</span><span style=\"font-size:14pt;font-family:宋体;\">日</span><span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p style=\"text-indent:168pt;\">\r\n	<span style=\"font-size:14pt;font-family:宋体;\">公示单位：金山区卫生和计划生育委员会</span><span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p style=\"text-indent:252pt;\">\r\n	<span style=\"font-size:14pt;\">2014</span><span style=\"font-size:14pt;font-family:宋体;\">年</span><span style=\"font-size:14pt;\">1</span><span style=\"font-size:14pt;font-family:宋体;\">月</span><span style=\"font-size:14pt;\">24</span><span style=\"font-size:14pt;font-family:宋体;\">日</span></p>','\0','M','Admin','2014-04-18 21:29:00','Admin','2014-04-19 07:17:48',NULL,NULL),(18,'上海华医堂中医门诊部设置公','<div id=\"divDisplay\" style=\"line-height:23px\" align=\"left\"><p style=\"text-align:center;\" align=\"center\">\r\n	<b><span style=\"font-family:宋体;font-size:16pt;\">公</span></b><b><span style=\"font-size:16pt;\">&nbsp;&nbsp;&nbsp; </span></b><b><span style=\"font-family:宋体;font-size:16pt;\">示</span></b><b><span style=\"font-size:16pt;\"></span></b> \r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p style=\"text-indent:28pt;\">\r\n	<span style=\"font-family:宋体;font-size:14pt;\">拟设“上海华医堂中医门诊部”。机构类别：专科门诊；执业地址：金山区朱泾镇文商路</span><span style=\"font-size:14pt;\">79</span><span style=\"font-family:宋体;font-size:14pt;\">号；诊疗科目：中医科、中西医结合科、医学检验科、医学影像科共</span><span style=\"font-size:14pt;\">4</span><span style=\"font-family:宋体;font-size:14pt;\">个一级科目；设置人：上海康臣医院投资管理有限公司；符合金山区《医疗机构设置规划》情况。</span><span style=\"font-size:14pt;\"></span> \r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\">&nbsp;&nbsp;&nbsp; </span><span style=\"font-family:宋体;font-size:14pt;\">公示日期：</span><span style=\"font-size:14pt;\">2012</span><span style=\"font-family:宋体;font-size:14pt;\">年</span><span style=\"font-size:14pt;\">10</span><span style=\"font-family:宋体;font-size:14pt;\">月</span><span style=\"font-size:14pt;\">10</span><span style=\"font-family:宋体;font-size:14pt;\">日</span><span style=\"font-family:宋体;font-size:14pt;\">至</span><span style=\"font-size:14pt;\">2010</span><span style=\"font-family:宋体;font-size:14pt;\">年</span><span style=\"font-size:14pt;\">10</span><span style=\"font-family:宋体;font-size:14pt;\">月</span><span style=\"font-size:14pt;\">17</span><span style=\"font-family:宋体;font-size:14pt;\">日</span><span style=\"font-size:14pt;\"></span> \r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p>\r\n	<span style=\"font-size:14pt;\"></span>\r\n</p>\r\n<p style=\"text-indent:224pt;\">\r\n	<span style=\"font-family:宋体;font-size:14pt;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;公示单位：金山区卫生局</span><span style=\"font-size:14pt;\"></span> \r\n</p>\r\n<span style=\"font-family:`Times New Roman`;font-size:14pt;\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 2012</span><span style=\"font-family:宋体;font-size:14pt;\">年</span><span style=\"font-family:`Times New Roman`;font-size:14pt;\">10</span><span style=\"font-family:宋体;font-size:14pt;\">月</span><span style=\"font-family:`Times New Roman`;font-size:14pt;\">10</span><span style=\"font-family:宋体;font-size:14pt;\">日</span></div>','\0','V','Admin','2014-04-18 21:51:48',NULL,NULL,'Admin','2014-04-19 01:00:06'),(19,'金山区住宿场所卫生等级公示名单  ','<blockquote style=\"margin: 0 0 0 40px; border: none; padding: 0px;\">\r\n\r\n\r\n\r\n\r\n\r\n\r\n<!--[if gte mso 9]><xml>\r\n <o:OfficeDocumentSettings>\r\n  <o:AllowPNG/>\r\n </o:OfficeDocumentSettings>\r\n</xml><![endif]-->\r\n\r\n<!--[if gte mso 9]><xml>\r\n <w:WordDocument>\r\n  <w:View>Normal</w:View>\r\n  <w:Zoom>0</w:Zoom>\r\n  <w:TrackMoves/>\r\n  <w:TrackFormatting/>\r\n  <w:PunctuationKerning