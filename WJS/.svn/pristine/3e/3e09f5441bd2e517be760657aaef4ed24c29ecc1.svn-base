INSERT INTO `supervision` VALUES (17,'加强诊所法规培训 提高依法执业意识  ','<table style=\"TABLE-LAYOUT: fixed; width: 91%;\" align=\"center\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\"><tbody><tr><td style=\"height: 28px\" width=\"100%\"><div id=\"divDisplay\" style=\"line-height:23px\" align=\"left\"><p style=\"text-indent: 24pt;\" align=\"left\">\r\n	<span style=\"font-family:仿宋_GB2312;font-size:12pt;\">一年一度的个体诊所校验工作即将开始，金山区卫监所在布置机构校验工作的同时，为了进一步规范个体诊所的医疗执业行为，结合以往工作中个体诊所存在的问题，以及新近颁布的法律规章对医疗机构、医务人员等有新的要求，金山区卫生监督所1月3日对辖区的34家个体诊所业主进行法律法规培训。</span>\r\n</p>\r\n<p style=\"text-indent: 24pt;\" align=\"left\">\r\n	<span style=\"font-family:仿宋_GB2312;font-size:12pt;\">培训主要采用列举以往监管中存在的问题，\r\n并对进行行政处罚的典型案例进行分析，如医师开具抗菌药物处方须经区卫生行政部门抗菌药物临床应用培训并考核合格，发布医疗广告须取得《医疗广告审查证\r\n明》并按照核准的广告样件发布医疗广告等，并针对性的学习法律法规中的条款要求及法律责任等，培训现场业主也就执业过程中存在的困惑与疑问等进行交流，业\r\n主纷纷表示此次培训受益匪浅。区卫生和计划生育委员会副主任吴靖平到会并讲话。<span style=\"color:#333333;\"></span></span>\r\n</p>\r\n<span style=\"font-family:仿宋_GB2312;color:#333333;font-size:12pt;\">在今后的日常监管中，卫生监督员会继续做好个体诊所的服务指导工作，针对存在的违法违规现象也会加大监管和处罚力度，进一步规范医疗服务市场，为人民群众提供一个安全就医环境。</span></div>\r\n                </td></tr>\r\n              <tr>\r\n                <td class=\"newstitle\" id=\"fontzoom\" style=\"height: 61px\" valign=\"top\"></td></tr></tbody></table>','\0','V','Admin','2014-04-19 21:34:51','Admin','2014-04-19 21:35:37','Admin','2014-04-19 21:35:37'),(18,'加强监管力度，规范消毒产品经营市场','<p style=\"text-indent:28pt;\">\r\n	<span style=\"font-family:仿宋_GB2312;font-size:14pt;\">为规范我区消毒产品经营市场，让广大消费者买到放心的消毒产品，保护消费者利益，我所执法三科已于2014年1月2日启动“金山区2014年元旦、春节期间消毒产品卫生监督检查方案”，依据方案要求对辖区内消毒产品经营单位实施专项监督检查。</span>\r\n</p>\r\n<p style=\"text-indent:35pt;\">\r\n	<span style=\"font-family:仿宋_GB2312;font-size:14pt;\">现已对辖区内经营消毒产品的2家大型超市进行了监督检查，重点检查了消毒剂和卫生用品（如84和妇女卫生用品等）。检查产品突出以市场小品牌、新品牌为主，检查内容包括</span><span style=\"font-family:宋体;font-size:14pt;\">经</span><span style=\"font-family:仿宋_GB2312;font-size:14pt;\">营单位是否索取生产企业卫生许可证、检查许可批件核对产品名称、型号或规格、生产企业、批准日期和有效期、产品说明、主要成分或部件等等。对于存在索证不齐全的单位，以约谈形式要求商场责任人来说明相关情况并予以相应的处理。</span>\r\n</p>\r\n<p style=\"text-indent:28pt;\">\r\n	<span style=\"font-family:仿宋_GB2312;font-size:14pt;\">下周，执法三科还将跟进监督检查工作，对相关经营单位销售的消毒产品进行监督抽检，如发现存在问题的将厉行处置，务必规范相关单位的经营行为，降低市场消毒产品的使用风险，保障好我区群众日常生活。</span></p>','\0','V','Admin','2014-04-19 21:35:40','Admin','2014-04-19 21:36:09','Admin','2014-04-19 21:36:09');
