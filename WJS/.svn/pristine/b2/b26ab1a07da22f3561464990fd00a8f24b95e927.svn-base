/*导航栏*/

#topNavMenu,#leftNavMenu{
    list-style: none;
    margin: auto;
}

#topNavMenu li{
    float: left;
    height: 30px;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
    margin-top: 4px; 
    padding-top: 3px;
}

#topNavMenu li.menu{
    width: 100px;
}

#topNavMenu li.menu:hover {
    background-image: url("./../img/tabtitle1.gif");
    background-repeat:no-repeat;
    background-size: 100% 100%;
}

#topNavMenu li.subMenu{
    width: 100px;
    background-color:#029CC2;
    color: white;
}

#topNavMenu li.subMenu:hover {
    background-image: url("./../img/tabtitle1.gif");
    background-repeat:no-repeat;
    background-size: 100% 100%;
}

.menuDropdownShow{
    display: block;
    width: 800px;
    height: 36px;
    margin-top: 13px;   
    background-color:transparent;   
    position: relative;
    z-index: 100;
}

.menuDropdownShow ul{
    list-style: none;
    margin-left: -48px;
}

.menuDropdownShow ul li{
    background-color:#029CC2;
    color: #045491;
}

.menuDropdownHide{
    display: none;
    position: relative;
    z-index: 100;
}

#leftNavMenu{
    float: left;
    font-size: 16px;
    font-weight: bold;
    text-align: left;
    padding-left:-0px; 
}

#leftNavMenu li.menu {
    height: 42px;
    width: 230px;
    background: url("../img/labelbg2.jpg") no-repeat 100% 100%;
    border-bottom: 1px #0288AB dotted;
    margin: 0px;
}

#leftNavMenu li.menuActive{
    color:white;
    height: 42px;
    width: 230px;
    background: url("../img/labelbg1.jpg") no-repeat 100% 100%;
    border-bottom: 1px #0288AB dotted;
    margin: 0px;
}

#leftNavMenu li div{
    padding-top:11px;
    padding-left: 20px;
}

