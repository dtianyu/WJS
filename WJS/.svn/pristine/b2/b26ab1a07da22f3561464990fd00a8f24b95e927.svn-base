/*设定panelGrid中Column的宽度,配合columnClasses使用*/

.panellist{
    margin: 0px 0px 0px 0px;
    width: 720px;
    min-height:420px;
}

.panellist .ui-datalist-item{
    margin: 0px 10px 10px -20px; padding: 0.2em;height: 24px; text-align: left;  
    border-bottom: 1px dotted #999999;
    white-space: nowrap;
    overflow: hidden;
    text-overflow:ellipsis;
    -ms-text-overflow: ellipsis;/* IE/Safari */
    -o-text-overflow: ellipsis;/* Opera */
    -moz-binding: url("ellipsis.xml#ellipsis");/*FireFox*/ 
}

.ui-datalist-item{
    margin: 0px 1px 1px -30px; padding: 0.1em;height: 16px;text-align: left; 
    white-space: nowrap;
    overflow: hidden;
    text-overflow:ellipsis;
    -ms-text-overflow: ellipsis;/* IE/Safari */
    -o-text-overflow: ellipsis;/* Opera */
    -moz-binding: url("ellipsis.xml#ellipsis");/*FireFox*/ 
}

