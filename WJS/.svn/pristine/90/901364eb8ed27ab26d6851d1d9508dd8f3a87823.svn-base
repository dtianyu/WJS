root { 
    display: block;
}

button{
    color: buttonhighlight;
    height: 22px;
}

button span{
    font-size:10px;
}

/*导航栏*/

#topTitleNavBar ul{
    padding-left: 150px;
}

#topNavMenu{
    margin: auto;
    padding-left: 150px;
}

#topNavMenu li{
    float: left;
    height: 30px;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
    margin-top: 4px; 
    padding-top: 3px;
}

#topNavMenu li.menu{
    width: 100px;
}

#topNavMenu li.menu:hover {
    background-image: url("./../img/tabtitle1.gif");
    background-repeat:no-repeat;
    background-size: 100% 100%;
}

#topNavMenu li.subMenu{
    width: 100px;
    background-color:#029CC2;
    color: white;
}

#topNavMenu li.subMenu:hover {
    background-image: url("./../img/tabtitle1.gif");
    background-repeat:no-repeat;
    background-size: 100% 100%;
}

.menuDropdownShow{
    display: block;
    width: 800px;
    height: 36px;
    margin-top: 13px;   
    background-color:transparent;   
    position: relative;
    z-index: 100;
}

.menuDropdownShow ul{
    /*    list-style: none;*/
    margin-left: -49px;
}

.menuDropdownShow ul li{
    background-color:#029CC2;
    color: #045491;
}

.menuDropdownHide{
    display: none;
}

.leftNavMenu{
    float: left;
    font-size: 16px;
    font-weight: bold;
    text-align: left;
    padding-left:-0px; 
    margin: auto;
}

.leftNavMenu li{   
    height: 36px;
    width: 230px;
    margin: 0px 0px 10px 0px;
    box-shadow: 1px 1px 2px black;   
}

.leftNavMenu li.active1 a{
    width: 200px;
    line-height: 2.3em;
    display: inline-block;
    padding-left: 30px;
    background: url("../img/labelbg1.jpg") no-repeat scroll left center;
    text-decoration :none;
}

.leftNavMenu li.style1 a{
    width: 200px;
    line-height: 2.3em;
    display: inline-block;
    padding-left: 30px;
    background: url("../img/labelbg2.jpg") no-repeat scroll left center;
    text-decoration :none;
}

.leftNavMenu li.style1 a:hover{
    background: url("../img/labelbg1.jpg") no-repeat scroll left center;
}

.leftNavMenu li.active2 a{
    width: 200px;
    line-height: 2.3em;
    display: inline-block;
    padding-left: 30px;
    background: url("../img/labelbg3.png") no-repeat scroll left center;
    text-decoration :none;
}

.leftNavMenu li.style2 a{
    width: 200px;
    line-height: 2.3em;
    display: inline-block;
    padding-left: 30px;
    background: url("../img/labelbg4.png") no-repeat scroll left center;
    text-decoration :none;
}

.leftNavMenu li.style2 a:hover{
    background: url("../img/labelbg3.png") no-repeat scroll left center;
}

/*导航栏*/

.box-shadow{
    box-shadow: 1px 1px 2px black;   
}

.more{
    float: right;padding-right: 5px;padding-top: 12px;
}

.nodisplay{
    display: none;
}

.nopadding{
    padding: 0px;
}

.pageDetail{
    width: 980px;
    min-height: 520px;
    margin: 0px 0px 0px 0px;
    padding-top: 20px;
    color: #000;
    background: none repeat scroll 0% 0% #FFF;
}

.pageContent{
    padding-left: 30px;
    padding-right: 30px;
}

.pageTitle{
    font-size: 14pt;font-weight: bold;
    text-shadow: 0px 1px 0px rgba(255, 255, 255, 0.8);
}

.quicklink{
    float: right;font-size: 12px;text-shadow: 0px 1px 0px rgba(255, 255, 255, 0.8);
    padding-right: 10px;
}

.ui-datalist-item{
    margin: 0px 1px 1px -30px; padding: 0.1em;height: 20px;text-align: left; 
    white-space: nowrap;
    overflow: hidden;
    text-overflow:ellipsis;
    -ms-text-overflow: ellipsis;/* IE/Safari */
    -o-text-overflow: ellipsis;/* Opera */
    -moz-binding: url("ellipsis.xml#ellipsis");/*FireFox*/ 
}

.ui-datatable-header {
    height: 22px;
}

.ui-datatable-footer  {
    height: 22px;
}

/*设定panelGrid显示样式*/

form .plgDialog{
    width: 980px; 
}

form .plgHead{
    margin: 0 0 20px 0;
    width: 730px;
}

form .plgHead tr{

}

form .plgHead td{

}

form .plgQuery{
    margin: 0 0 20px 0;
    width: 730px;
    background: transparent;
}

