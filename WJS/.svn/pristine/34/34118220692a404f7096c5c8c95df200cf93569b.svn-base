#slides {
    position:relative;  
    width: 345px;
    display:none;
    
}

#slides img{
    width: 345px;
    height: 250px;
}

.slidesjs-container{
}

.slidesjs-navigation{position: relative;z-index: 999;float:right;margin-top:-18px;padding-right:15px;}

.slidesjs-navigation a{font:bold 20px/normal simsun;text-decoration:none;color:#333;}
.slidesjs-navigation a:hover{color:#C00}
.slidesjs-previous{}
.slidesjs-next{}

.slidesjs-pagination{position: relative;z-index: 999;float:left;margin-top:-15px;padding-left:15px; }
.slidesjs-pagination li{float:left;padding:0 3px;}
.slidesjs-pagination li a{display:block;width: 13px;height:0;padding-top:13px;background:url("pagination.png") 0 0;overflow:hidden;}
.slidesjs-pagination li a.active,.slidesjs-pagination li a:hover.active {background-position: 0 -13px}
.slidesjs-pagination li a:hover {background-position: 0 -26px}