root { 
    display: block;
}

button{
    color: buttonhighlight;
    height: 22px;
}

button span{
    font-size:10px;
}

/*用户登录框*/

#loginDailog{
    height: 180px;
    width: 400px;
    margin: auto;
    background-color:transparent;
    color:white;
}

form .btnLogout{
    background-color: orange;  
    float: right;
}

form .btnLogin{
    height: 35px;
    width: 400px;
    text-align: center;
    background-color: #ff6666;
}

form .btnLogin span{
    font-size: 14px;
}

form .plglogin{
    width: 400px;
    margin-top: 100px;
    background: transparent none repeat 0% 0%;
}

form .plglogin tr{
    height: 65px;
    border:none;
    background-color: transparent;
}

form .plglogin td{
    border: none;
}

form .plgLoginLabel{
    width: 100px;
    text-align: right;
}

form .plgLoginTnput input{
    width: 200px;
}

.currentUserLable{
    border: none;
    color: black;
    font-weight: bold; 
    width: 60px;
    float: right;
    padding: 5px;
}

.currentUserName{
    border:none;
    color: black;
    font-weight: bold; 
    width: 80px;
    float: right;
    padding: 5px;
}

/*用户登录框*/

form .plgDialog{
    width: 850px; 
}

form .plgHead{
    margin: 0 0 20px 0;
    width: 980px;
}

form .plgHead tr{

}

form .plgHead td{

}
/*设定表头panelGrid*/
form .plgQuery{
    margin: 0 0 20px 0;
    width: 980px;
}

form .plgQuery tr{
    border: none; 
}

form .plgQuery td{
    border: none; 
}
/*设定查询条件panelGrid*/

form .plgLable{
    width: 60px;
    text-align: right;
}

form .plgInput {
    width: 220px;
}

form .plgInputLong {
    width: 520px;
}

form .plgInputLong input{
    width: 420px;
}

/*设定panelGrid中Column的宽度,配合columnClasses使用*/

.panellist{
    margin: 0px 0px 0px 0px;
    width: 760px;
    min-height:420px;
}

.panellist .ui-datalist-item{
    margin: 0px 10px 10px -20px; padding: 0.2em;height: 20px; text-align: left;    
}

.ui-datalist-item{
    margin: 0px 1px 1px -30px; padding: 0.1em;height: 14px;
    text-align: left; 
    white-space:nowrap;
    overflow: hidden;
    text-overflow:ellipsis;
    -ms-text-overflow: ellipsis;/* IE/Safari */
    -o-text-overflow: ellipsis;/* Opera */
    -moz-binding: url("ellipsis.xml#ellipsis");/*FireFox*/ 
}

.tbllist{
    margin: 0px 0px 0px 0px;
    width: 750px;
}

.plgUpload{
    margin: 20px 0px 0px 0px;
    width: 750px; 
}

.ui-datatable-header {
    height: 22px;
}

.ui-datatable-footer  {
    height: 22px;
}

.ui-panelgrid  tr {
    height:  30px;
}

.ui-panelmenu-header{
    height: 30px;
    background-image: url("../img/titlebg.gif");
    background-repeat: no-repeat;
    background-size: 100% 100%; 
    font-size: 12px;
    padding-left: 12px;
}

